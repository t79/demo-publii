{{> head}}



{{#post}}

<article>
<header>
<h1>{{{title}}}</h1>
<p>Posted <time datetime="{{date createdAt 'YYYY-MM-DDTHH:mm'}}">{{date createdAt}}</time>
{{#if tags}}{{#each tags}}<a href="{{url}}">{{name}}</a>{{/each}}{{/if}}</p>
</header>

<main>

{{{ text }}}

</main>

</article>


{{/post}}

{{> footer}}